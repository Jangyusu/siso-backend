{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@config/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@config/config": "<rootDir>/../libs/config/src",
    "@app/document/(.*)": "<rootDir>/../libs/document/src/$1",
    "@app/document": "<rootDir>/../libs/document/src"
  }
}